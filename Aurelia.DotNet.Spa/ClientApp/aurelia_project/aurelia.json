{
  "name": "Aurelia.DotNet.Spa",
  "type": "project:application",
  "bundler": {
    "id": "webpack",
    "displayName": "Webpack"
  },
  "httpProtocol": {
    "id": "http1",
    "displayName": "HTTP/1.1"
  },
  "build": {
    "options": {
      "server": "dev",
      "extractCss": "prod",
      "coverage": false
    }
  },
  "platform": {
    "id": "web",
    "displayName": "Web",
    "hmr": false,
    "open": false,
    "port": 8080,
    "output": "dist"
  },
  "loader": {
    "id": "none",
    "displayName": "None"
  },
  "transpiler": {
    "id": "typescript",
    "displayName": "TypeScript",
    "fileExtension": ".ts"
  },
  "markupProcessor": {
    "id": "none",
    "displayName": "None",
    "fileExtension": ".html"
  },
  "cssProcessor": {
    "id": "none",
    "displayName": "None",
    "fileExtension": ".css"
  },
  "jsonProcessor": {
    "id": "none",
    "displayName": "None",
    "fileExtension": ".json"
  },
  "editor": {
    "id": "vscode",
    "displayName": "Visual Studio Code"
  },
  "features": {
    "navigation": "navigation",
    "aurelia-fetch-client": "aurelia-fetch-client",
    "bootstrap": "bootstrap",
    "jquery": "jquery",
    "font-awesome": "font-awesome"
  },
  "unitTestRunner": {
    "id": "none",
    "displayName": "None"
  },
  "integrationTestRunner": {
    "id": "none",
    "displayName": "None"
  },
  "paths": {
    "root": "src",
    "resources": "resources",
    "elements": "resources/elements",
    "attributes": "resources/attributes",
    "valueConverters": "resources/value-converters",
    "bindingBehaviors": "resources/binding-behaviors"
  }
}
